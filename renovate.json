{
  "customManagers": [
    {
      "customType": "regex",
      "description": "Regex manager to update the spring major version in `spring-boot-major`.",
      "fileMatch": [
        "spring-boot-major$"
      ],
      "matchStrings": [
        "^(?<currentValue>\\d+)"
      ],
      "datasourceTemplate": "maven",
      "depNameTemplate": "org.springframework.boot:spring-boot"
    }
  ],
  "packageRules": [
    {
      "matchPackageNames": ["org.springframework.boot:spring-boot"],
      "matchFileNames": ["spring-boot-major"],
      "extractVersion": "^(?<version>\\d+)"
    }
  ]
}
